{
  "root": true,
  "extends": [
    "airbnb-base",
    "fdio/base",
    "prettier"
  ],
  "globals": {
    "__DEV__": true,
    "__WECHAT__": true,
    "__ALIPAY__": true,
    "App": true,
    "Page": true,
    "Component": true,
    "Behavior": true,
    "wx": true,
    "getApp": true,
    "getCurrentPages": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-continue": 0,
    "no-restricted-globals": 0,
    "implicit-arrow-linebreak": 0,
    "prefer-rest-params": 0,
    "vars-on-top": 0,
    "object-shorthand": 0,
    "one-var-declaration-per-line": 0,
    "no-control-regex": 0,
    "no-case-declarations": 0,
    "import/no-dynamic-require": 0,
    "consistent-return": 0,
    "member-ordering": 0,
    "error-body-style": 0,
    "no-template-curly-in-string": 0,
    "camelcase": "off"
  }
}
