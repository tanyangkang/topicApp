/* pages/billboard/index.wxss */
page
{
  background: #f2f2f2;
}
.fastNavItem
{
  width:100%;
  height:70px;
  text-align: center;
  font-size:12px;
  border: none !important;
  border-radius: 0;
  line-height: inherit;
}
.fastNavItem-hover
{
  background: #f2f2f2 !important;
}
.fastNavItem > .title
{
  font-size:12px;
  margin-top:-5px;
}
.fastNavItem > .iconfont
{
  font-size:22px;
  margin-top:5px;
}
.backtop-float {
  position: fixed;
  right: 20rpx;
  bottom: 40rpx;
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  border-radius: 50%;
  color: #fff;
  font-size: 2em;
  line-height: 46px;
  font-weight: normal;
  background: linear-gradient(#ff8a00, #e52e71);
}
